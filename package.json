    "vite": "^5.1.5",
    "vite-plugin-electron": "^0.28.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "ali-oss": "^6.20.0",
    "better-sqlite3": "^9.4.5",
